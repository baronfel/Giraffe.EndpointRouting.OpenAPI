    "version": "0.2.0",
    "configurations": [
        {
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/sample/OpenAPI.Sample/bin/Debug/net5.0/OpenAPI.Sample.dll",
            "cwd": "${workspaceFolder}/sample/OpenAPI.Sample",
            "name": "Launch",
            "justMyCode": false,
            "enableStepFiltering": false,
            "sourceLinkOptions": {
                "*": {
                    "enabled": true
